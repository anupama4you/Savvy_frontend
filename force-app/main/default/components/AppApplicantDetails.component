            <td colspan="4" class="tableCaption"><strong>
                <apex:outputText value="Applicant {!numApplicant}" rendered="{!app.Application_Type__c=='Personal'}"/>
                <apex:outputText value="Director {!numApplicant}" rendered="{!app.Application_Type__c=='Business'}"/>
