    <apex:attribute name="secondJobApplic1" type="Application_Employment__c" description="Application Employment"/>
    <apex:attribute name="isSecondJobApplic1" type="boolean" description="Applicant Employment"/> 
    <apex:attribute name="secondJobApplic2" type="Application_Employment__c" description="Application Employment"/>
