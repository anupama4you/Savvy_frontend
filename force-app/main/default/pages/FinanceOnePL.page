    <c:sidebarV2 oppId="{!o.Id}" appId="{!o.Application__c}" showLenders="true"/>
    <div id="FinanceOne">
        <apex:form id="FinanceOneForm">
            <apex:pageMessages id="pageMessage" />
            <!-- Send presentation -->
            <apex:actionFunction action="{!sendPresentationStep1}" name="sendPresentationStep1" reRender="pageMessage" status="stsProd" oncomplete="sendPresentationStep2();"/>
            <apex:actionFunction action="{!sendPresentationStep2}" name="sendPresentationStep2" status="stsProd2" reRender="pageMessage" oncomplete="sendPresentationStep3();"/>
            <apex:actionFunction action="{!sendPresentationStep3}" name="sendPresentationStep3" status="stsProd2" reRender="pageMessage, pnlInsProd"/>
            
            <!-- Send Quote -->
            <apex:actionFunction action="{!sendCustomerEmail1}" name="sendCustomerEmail1" reRender="pageMessage" status="stsSend1" oncomplete="sendCustomerEmail2();"/>
            <apex:actionFunction action="{!sendCustomerEmail2}" name="sendCustomerEmail2" status="stsSend2" reRender="pageMessage"/>
            <!-- End -->
            
            <apex:actionFunction action="{!ericLPICalculate}" name="ericLPICalculate" reRender="realtimeNaf, netDeposit, pageMessage, fldPremium, fldCommission" status="stsNAF" />
            
            <!--<apex:actionFunction action="{!realtimeNafCalc}" name="nafRefresh" reRender="realtimeNaf" />-->
            <apex:actionFunction action="{!baseRateCalc}" name="baseRateRefresh" reRender="baseRate, maxRate" />
            <!-- <apex:actionFunction action="{!warrantyPriceCalc}" name="warrantyRefresh" oncomplete="riskFeeRefresh();dofRefresh();" reRender="warranty,pageMessage" />
            <apex:actionFunction action="{!warrantyIncomeCalc}" name="warrantyIncRefresh" reRender="warrantyIncome,pageMessage" /> -->
            <apex:actionFunction action="{!riskFeeCalc}" name="riskFeeRefresh" reRender="riskFee,riskFeeTotal,realtimeNaf,netDeposit,pnlInsProd" status="stsNAF" />
            <apex:actionFunction action="{!applicationFeeCalc}" name="applicationFeeRefresh" reRender="applicationFee,riskFee,riskFeeTotal,realtimeNaf,netDeposit,pnlInsProd" status="stsNAF" oncomplete="riskFeeRefresh();" />

            <apex:actionFunction action="{!dofCalc}" name="dofRefresh" reRender="dof" oncomplete="riskFeeRefresh();" />

            <div class="titleDiv">
                <h1>Finance One - PL for {!Custom_Opportunity__c.Name}</h1>
                <div class="buttonPanel">
                    <table>
                        <tr>
                            <td>
                                <apex:actionStatus startText="(Processing...)" id="stsprocessing" />
                                <apex:actionStatus startText="(Saving...)" id="stsSend1" />
                                <apex:actionStatus startText="(Sending...)" id="stsSend2" />&nbsp;&nbsp;&nbsp;

                                <apex:commandButton styleClass="calculateButtons" value="Calculate" action="{!calculate}" status="stsprocessing" reRender="FinanceOneForm" />
                                <apex:commandButton styleClass="calculateButtons" value="Reset" action="{!reset}" status="stsprocessing" reRender="FinanceOneForm" />
                                <apex:commandButton styleClass="calculateButtons" value="Save Quoting" action="{!saveQuoting}" status="stsprocessing" reRender="FinanceOneForm"/>
                                <apex:commandButton styleClass="calculateButtons" value="Save Pre-approval" action="{!savePreApproval}" status="stsprocessing" reRender="FinanceOneForm"/>
                                <apex:commandButton styleClass="calculateButtons" value="Save Amendment" action="{!saveAmendment}" status="stsprocessing" reRender="FinanceOneForm"/>
                                <apex:commandButton styleClass="calculateButtons" value="Save Formal Approval" action="{!saveFormalApproval}" status="stsprocessing" reRender="FinanceOneForm"/>
                                
                                <input name="DoAction" type="button" value="Send Quote" class="calculateButtons"
                                       onclick="sendCustomerEmail1();return false;"/>   

                            </td>
                            <td>
                                <apex:image styleClass="logo" url="{!$Resource.FinanceOneLogo}" />
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
            <div class="calculatorInput"  >
                <div class="divTitle">
                    <h3>Input</h3>
                </div>
                <div id="leftTables">
                    <div id="financeTab"><h3>Finance Details</h3></div>
                    <table id="financeDetails">
                        <tr>
                            <td class="financeColumn">
                            <apex:outputLabel styleClass="calcLabel" value="Loan Type" for="LoanType" />
                            </td>
