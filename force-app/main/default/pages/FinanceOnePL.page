                            <apex:selectList label="Loan Type" value="{!category}" multiselect="false" size="1" styleClass="selectList" id="category" onchange="baseRateRefresh();">
                                <apex:selectOption itemLabel="--None--" itemValue="" />    
                                <apex:selectOption itemLabel="Secured" itemValue="Secured" />
                                <apex:selectOption itemLabel="Unsecured" itemValue="Unsecured" />
                            </apex:selectList>
                            </td>
                        </tr>
                        <tr>
                            <td class="financeColumn">
                            <apex:outputLabel styleClass="calcLabel" value="Finance Amount" for="carPrice" />
                            </td>
