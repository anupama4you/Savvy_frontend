        <script>
          jQuery.noConflict();
          jQuery(function () {
            jQuery(".video").click(function () {
              var theModal = jQuery(this).data("target"),
                videoSRC = jQuery(this).attr("data-video"),
                videoSRCauto = 'https://www.youtube.com/embed/' + videoSRC + "?modestbranding=1&rel=0&controls=0&showinfo=0&html5=1&autoplay=1";
              jQuery(theModal + ' iframe').attr('src', videoSRCauto);
              jQuery(theModal + ' button.close').click(function () {
                jQuery(theModal + ' iframe').attr('src', videoSRC);
              });
            });
          });
        </script>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.3/assets/owl.carousel.min.css" rel="stylesheet"/>
        <link href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.3/assets/owl.theme.default.css" rel="stylesheet"/>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.3/owl.carousel.min.js"></script>
        <script>
          var owl_is_loaded = false;
          var owl;
          function go_up() {
            jQuery('html, body').animate({ scrollTop: 0 }, 500, 'swing');
          }

          var init_owl = function (owl, w) {
            jQuery('.owl-carousel').on('initialized.owl.carousel', function (event) {
              owl_is_loaded = true;
            });
            owl.owlCarousel({
              items: 4,
              margin: 0,
              nav: false,
              dots: false,
              mouseDrag: false,
              touchDrag: false,
              responsive: {
                0: {
                  items: 1,
                  mouseDrag: false,
                  touchDrag: true,
                  nav: true,
                },
                1024: {
                  items: 4,
                }
              }
            });

            owl.on('changed.owl.carousel', function (event) {
              var owl_data = owl.data('owlCarousel');
              var w = window.innerWidth;
              var owl_index = event.item.index;
              if (w < 768) {
                var h = jQuery('.owl-item').eq(owl_index).height();
                if (owl_index > 0) {
                  h += 50;
                } else {
                  h += 40;
                }
                owl.css('height', h + "px");
              } else {
                owl.removeAttr('style');
              }
            });

            if (w > 767) {
              owl.owlCarousel('destroy');
              owl.addClass('owl-loaded');
              owl_is_loaded = false;
            }
          }

          jQuery.noConflict();
          jQuery(function () {
            var w = window.innerWidth;
            owl = jQuery('.owl-carousel');
            init_owl(owl, w);
          });

          function reloadOwl() {

            var w = window.innerWidth;

            owl = jQuery('.owl-carousel');
            go_up();
            if (jQuery('.message.warningM3').is(":visible")) {
              init_owl(owl, w);
            }
          }

          function reloadOwlOnBack() {
            go_up();
            var w = window.innerWidth;
            owl = jQuery('.owl-carousel');
            init_owl(owl, w);
          }

          jQuery(window).resize(function () {
            var w = window.innerWidth;
            console.log('w: ' + w);
            console.log('owl_is_loaded: ' + owl_is_loaded);
            if (w >= 768) {
              if (owl_is_loaded) {
                owl.owlCarousel('destroy');
                owl.addClass('owl-loaded');
                owl_is_loaded = false;
              }
            } else {
              owl = jQuery('.owl-carousel');
              if (!owl_is_loaded) {
                init_owl(owl, w);
              }
              owl.on('resized.owl.carousel', function (event) {
                var h = jQuery('.owl-item.active').height() + 50;
                console.log('h: ' + h);
                jQuery('.owl-carousel').css('height', h + "px");
              });
            }
          });
        </script>
