                <h1>Savvy Finance Lead Servicing Calculator for {!o.Name}</h1>
                
                <apex:outputPanel layout="block" styleClass="toolOptions">
                    <apex:actionStatus startText="processing... " id="genPdfDocument"  />
                    <!--<apex:commandButton value="Calculate" action="{!servCalculateV2}" styleClass="commandButtons" rendered="false" />-->
                    <apex:commandButton value="Save Calculation" action="{!ServicingPdf2}" styleClass="commandButtons" reRender="msgServicingCalc" status="genPdfDocument" />
                    <!--<apex:commandButton value="Save Pre-approval" action="{!sericingSavePreApproval}" styleClass="commandButtons" reRender="msgServicingCalc" status="genPdfDocument" />-->
                </apex:outputPanel>
            </apex:outputPanel> 
        <apex:pageBlock mode="edit" id="servicingCalc">
            
            <!-- SECTION: INITIAL PARAMETERS -->
