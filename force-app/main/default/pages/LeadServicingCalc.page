    <div id="ServicingCalc">
        <apex:pageMessages id="msgServicingCalc" />    
        <apex:variable value="{!Lead__c}" var="o" />
          <apex:form styleClass="appForm" id="servicingcalc">
            <apex:actionFunction action="{!servCalculateTotalNetMonthlyIncome}" name="servCalculateTotalNetMonthlyIncome" 
                                 reRender="fldCalcTotNetMonthlyIncome, fldCalcTotAmountLoanPayment" status="totalAmountStatus" />
            <apex:actionFunction action="{!servCalculateRentMtgePaid}" name="servCalculateRentMtgePaid" 
                                 reRender="fldCalcTotRentMtgPaid, fldCalcTotAmountLoanPayment, fldCalcNetPartnerIncome, msgServicingCalc, fldTotRentMorg" status="totalAmountStatus"/>
            <apex:actionFunction action="{!servCalculateCentreLink}" name="servCalculateCentreLink" status="totalAmountStatus"
                                 reRender="fldCalcCLinkIncome, fldCalcCLinkTotalIncome, fldCalcCLinkIncomePercentage, fldCalcCLinkAnnualToLoannet, fldCalcTotAmountLoanPayment" />
            <apex:actionFunction action="{!servCalculateCreditCard}" name="servCalculateCreditCard" 
                                 reRender="fldCalcTotOtherDebts, fldCalcCCardMonthlyPayment, fldCalcTotAmountLoanPayment" status="totalAmountStatus" />
            <apex:actionFunction action="{!servCalculateLiabilities}" name="servCalculateLiabilities" 
                                 reRender="fldCalcTotLiabilities, fldCalcTotAmountLoanPayment, msgServicingCalc" status="totalAmountStatus" />
              <!--  
            
            
            
            
            
            <apex:actionFunction action="{!calculateAmountAvalilableLoanPayment}" name="calculateAmountAvalilableLoanPayment" 
                                 reRender="fldCalcTotAmountLoanPayment, msgServicingCalc" status="totalAmountStatus" />
            -->
