                                        <td class="labelColumn">State</td>
                                        <td class="dataColumn">
                                            <apex:selectList value="{!dto.state}" multiselect="false" size="1" styleClass="selectList"
                                                             disabled="{!dto.lender != 'GEO'}" id="fldCalcState">
                                                <apex:actionSupport event="onchange" action="{!servCalculateLivingExpenses}" reRender="fldCalcRentPayAmount, pnltotals" status="totalAmountStatus" />
