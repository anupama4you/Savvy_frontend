                                            <apex:selectList value="{!dto.maritalStatus}" multiselect="false" size="1" styleClass="selectList">
                                                <apex:actionSupport event="onchange" action="{!servCalculateLivingExpenses}" reRender="fldCalcRentPayAmount, fldCalcNetPartnerIncome, pnltotals" status="totalAmountStatus"/>
