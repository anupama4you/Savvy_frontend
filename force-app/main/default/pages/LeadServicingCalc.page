                                </apex:inputCheckbox>Split
                                &nbsp;&nbsp;&nbsp;<i>Total:&nbsp;
                                <apex:outputText value="({0, Number, Currency})" id="fldTotLivExpenses">
                                    <apex:param value="{!dto.totLivingExpenses}" />
                                </apex:outputText></i>
                            </td>
                        </tr>
