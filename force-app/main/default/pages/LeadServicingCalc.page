                                <apex:outputText value="{0, Number, Currency}" id="fldCalcTotAmountLoanPayment">
                                    <apex:param value="{!dto.totAmountLoanPayment}" />
                                </apex:outputText></h3>&nbsp;&nbsp;&nbsp;
                                <apex:actionStatus startText="(calculating...)" id="totalAmountStatus"  />
                            </td>
                            <td>&nbsp;</td>
                        </tr>
                    </table>
                    <p>&nbsp;</p>
                </apex:outputPanel>
            </apex:outputPanel>    
            
