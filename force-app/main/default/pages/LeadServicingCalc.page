                            <td>Credit Cards</td>
                            <td class="notIncomeValue">
                                <apex:outputText value="{0, Number, Currency}" id="fldCalcTotOtherDebts">
                                    <apex:param value="{!dto.totOtherDebts}" />
                                </apex:outputText>
