                    <apex:selectOption itemLabel="--None--" itemValue="" />
                    <apex:selectOption itemLabel="Car" itemValue="Car" />
                    <apex:selectOption itemLabel="Bikes / Scooters" itemValue="Bikes / Scooters" />
                    <apex:selectOption itemLabel="Boats / Personal Watercraft" itemValue="Boats / Personal Watercraft" />
                    <apex:selectOption itemLabel="Caravans / Motorhomes" itemValue="Caravans / Motorhomes" />
                  </apex:selectList>
                </td>
              </tr>
              <tr>
                <td class="financeColumn">
                  <apex:outputLabel styleClass="calcLabel" value="Asset Condition" for="assetCondition" />
                </td>
                <td class="financeColumn">
                  <apex:selectList label="Loan Type" value="{!assetCondition}" multiselect="false" size="1" styleClass="selectList" id="assetCondition">
                    <apex:actionSupport action="{!resetCarAge}" event="onchange" reRender="carAge, maxRate, baseRate" />
                    <apex:selectOption itemLabel="--None--" itemValue="" />
                    <apex:selectOption itemLabel="New/Demo" itemValue="New/Demo" />
                    <apex:selectOption itemLabel="Used" itemValue="Used" />
