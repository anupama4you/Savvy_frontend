<apex:page standardController="Custom_Opportunity__c" extensions="GenerateAppPdfExtension" applyBodyTag="false" renderAs="pdf">
    <apex:stylesheet value="{!$Resource.AppPdfCss}"/>
    <div class="header">
        <table style="border: 0px;" width="100%">
            <tr style="border: 0px;" width="100%">
                <td style="border: 0px;" width="30%" align="left">
                  <apex:outputPanel rendered="{!ISBLANK(AccountVar.Business_Logo__c)}">
                    <!--Display default Logo if Portal User has no logo.-->
                    <apex:image url="{!$Resource.newSavvyLogo}" alt="Logo" />
                  </apex:outputPanel>
                  <apex:outputPanel rendered="{!NOT(ISBLANK(AccountVar.Business_Logo__c))}">
                    <!--Display Portal User's Logo if available-->
                    <apex:image id="Logo" value="{!AccountVar.Business_Logo__c}" height="60" />
                  </apex:outputPanel>
                </td>
                <td style="border: 0px;" width="70%" align="left">
                    <p>
                      <span style="font-size:1.5em;">FINANCE APPLICATION</span><br/>
                      <!-- <span style="font-size:0.8em;"><span style="color:#ee245f;">E</span> applications@savvy.com.au <font>|</font> <span style="color:#ee245f;">P</span> 1300 974 066 <font>|</font> <span style="color:#ee245f;">F</span> 1300 978 066<br/>
                      <span style="color:#ee245f;">A</span> Level 1, 145 The Parade, Norwood, SA 5067 <font>|</font> <span style="color:#ee245f;">www</span>.savvy.<span style="color:#ee245f;">com.au</span></span> -->
                  </p>
                </td>
            </tr>
        </table>
    </div>
    <div class="footer">
        <div class="footerContent">
            Page <span class="pagenumber"/> of <span class="pagecount"/>&nbsp;&nbsp;&nbsp;&nbsp;
            <apex:outputText value="{0, date, dd/MM/yyyy HH:mm:ss}" label="Date" styleClass="dataReadOnly">
                <apex:param value="{!NOW()+offset}" />
            </apex:outputText>
        </div>
    </div>
    <div class="content">
        <apex:variable value="{!Custom_Opportunity__c}" var="o" />
        <!-- Notes -->
        <apex:outputPanel id="notesSection" layout="block" rendered="{!isPreApproval && notes.size > 0}">
            <h3>{!NumberForNotes}.) Notes:</h3>
            <table class="tableSubsection" width="100%">
                <tr>
                    <th width="25%">Last Modified</th>
                    <th width="75%">Note</th>
                </tr>
                <apex:repeat value="{!notes}" var="n" >
                    <tr class="{!IF(n.Category__c=='Lender','lenderComment','')}">
                        <td>
                            <apex:outputText value="{0, date, dd/MM/yyyy hh:mm}">
                                <apex:param value="{!n.LastModifieddate}"/>
                            </apex:outputText>, &nbsp;
                            <apex:outputText value="{!n.Owner.Name}"/>
                        </td>
                        <td>
                            <b><apex:outputText value="{!n.Type_of_Data__c}"/>&nbsp;To&nbsp;<apex:outputText value="{!n.Category__c}"/>:</b><br/>                    
                            <apex:outputText value="{!n.Data__c}"/>
                        </td>
                    </tr>
                </apex:repeat>
            </table>
        </apex:outputPanel>
        
        <apex:outputPanel layout="block" rendered="{!isPreApproval && (notes == null || notes.size == 0)}">
            <h3>{!NumberForNotes}.) Notes: (none)</h3>
        </apex:outputPanel>
        
